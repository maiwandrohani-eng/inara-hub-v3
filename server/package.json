{
  "name": "inara-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx dotenv-cli -e .env -- tsx watch src/index.ts",
    "build": "tsc --skipLibCheck",
    "start": "node dist/index.js",
    "postinstall": "npm run db:generate 2>/dev/null || true",
    "db:generate": "prisma generate --schema ./prisma/schema.prisma",
    "db:push": "npx dotenv-cli -e .env -- prisma db push --schema ./prisma/schema.prisma",
    "db:migrate": "npx dotenv-cli -e .env -- prisma migrate dev --schema ./prisma/schema.prisma",
    "db:studio": "npx dotenv-cli -e .env -- prisma studio --schema ./prisma/schema.prisma",
    "db:seed": "npx dotenv-cli -e .env -- tsx src/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@prisma/client": "^5.7.1",
    "@types/pdfkit": "^0.17.4",
    "@vercel/node": "^3.0.7",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/pdf-parse": "^1.1.5",
    "dotenv-cli": "^11.0.0",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
